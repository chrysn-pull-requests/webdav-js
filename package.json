{
  "name": "webdav-js",
  "version": "2.0.0-beta1",
  "description": "WebDAV functionality intended for use as a bookmarklet or to make a simple Apache webserver an interactive WebDAV environment.",
  "repository": {
    "type": "git",
    "url": "git@github.com:dom111/webdav-js.git"
  },
  "keywords": [
    "webdav",
    "bookmarklet"
  ],
  "author": "Dom Hastings",
  "license": "MIT",
  "scripts": {
    "build:examples:branch": "bash build/examples-branch.sh",
    "build:examples": "bash build/examples.sh",
    "build:sass": "bash build/sass.sh",
    "build": "yarn run build:examples && yarn run build:sass && yarn webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "jasmine": "^3.5.0",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "preprocessor": "^1.4",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser": "^4.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "basiclightbox": "^5.0.2",
    "melba-toast": "0.2.0",
    "prismjs": "^1.17.1"
  }
}
